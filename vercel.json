{
  "version": 2,
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "wsgi.py"
    }
  ],
  "env": {
    "NEWS_API_KEY": "@news_api_key",
    "FLASK_CONFIG": "production",
    "SECRET_KEY": "@secret_key"
  }
}
